{
  "arrowParens": "always",
  "bracketSpacing": true,
  "cursorOffset": -1,
  "endOfLine": "lf",
  "importOrder": [
    "^react$",
    "@sentry/nextjs",
    "@sentry/",
    "^@testing-library",
    "^prop-types$",
    "^next",
    "^@tanstack",
    "<THIRD_PARTY_MODULES>",
    "useMediaQuery$",
    "^@mui/material/styles$",
    "^@mui/material",
    "^@junyiacademy/ui$",
    "^@common-components$",
    "^@shared-utils",
    "models",
    "contexts",
    "hooks",
    "/use",
    "utils",
    "styles",
    "components",
    "^[./]"
  ],
  "importOrderSortSpecifiers": true,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false
}
